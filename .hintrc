{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/forms": [
      "default",
      {
        "select-name": "off",
        "label": "off"
      }
    ],
    "no-inline-styles": "off",
    "meta-viewport": "off",
    "meta-charset-utf-8": "off",
    "axe/parsing": [
      "default",
      {
        "duplicate-id-active": "off"
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "frame-title": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "scrollbar-width",
          "scrollbar-width: thin",
          "-webkit-overflow-scrolling",
          "backdrop-filter"
        ]
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off"
      }
    ]
  }
}